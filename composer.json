{
    "name": "vita/laravel simple file manager",
    "description": "Manager file as simple way in Laravel",
    "keywords": [
        "vita",
        "laravel simple file manager"
    ],
    "homepage": "https://github.com/vita/laravel simple file manager",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Viet Tuan",
            "email": "viet.tuan.9210@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vita\\LaravelSimpleFileManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vita\\LaravelSimpleFileManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vita\\LaravelSimpleFileManager\\LaravelSimpleFileManagerServiceProvider"
            ],
            "aliases": {
                "LaravelSimpleFileManager": "Vita\\LaravelSimpleFileManager\\LaravelSimpleFileManagerFacade"
            }
        }
    }
}
